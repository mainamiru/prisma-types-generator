{
  "compilerOptions": {
    "module": "CommonJS",
    "declaration": true,
    "noEmit": false,
    "allowJs": true,
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false
  },
  "exclude": ["./node_modules", "./dist"]
}
